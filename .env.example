BANKING_APP_NAME=Banking Service
BANKING_API_PREFIX=/api/v1
BANKING_API_VERSION=0.1.0
# For Docker (matches docker-compose services). Fill in user:password@ if you change defaults.
BANKING_DATABASE_URL=postgresql+asyncpg://app:app@db:5432/banking_db

# Postgres settings (used by docker-compose)
POSTGRES_USER=app
POSTGRES_PASSWORD=app
POSTGRES_DB=banking_db

# pgAdmin settings (used by docker-compose)
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin
